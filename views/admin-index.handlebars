 <div class="container">

        <table class="centered striped responsive-table">
            <thead>
                <tr>
                    <th data-field="fullname">Full Name</th>
                    <th data-field="username">User Name</th>
                    <th data-field="email">Email</th>
                    <th data-field="bio">Biography</th>
                    <th data-field="reputation">Reputation</th>
                </tr>
            </thead>
            <tbody>
                {{#each user_data}}
                <tr>
                 <td>   
                {{this.full_name}}
                 </td>
                 <td>
                {{this.username}}
                 </td>
                 <td>
                {{this.email}}
                 </td>
                 <td>
                {{this.bio}}
                 </td>
                 <td>
                     {{this.reputation}}
                 </td>
                </tr>
                {{/each}}
            </tbody>
        </table>
  </div>